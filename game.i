  if (b->direction == 0) {
   if (b->row + b->height > 0 && b->col + b->width > 0 && b->row < 160 && b->col < 240) {
    b->col -= 2;
   } else {
    b->active = 0;
   }
  }

  else if (b->direction == 1) {
   if (b->row + b->height > 0 && b->col + b->width > 0 && b->row < 160 && b->col < 240) {
    b->col += 2;
   } else {
    b->active = 0;
   }
  }

   else if (b->direction == 2) {
   if (b->row + 2* b->height > 0 && b->col + b->width > 0 && b->row < 160 && b->col < 240) {
    b->row -= 2;
   } else {
    b->active = 0;
   }
  }

  else if (b->direction == 3) {
   if (b->row + b->height > 0 && b->col + b->width > 0 && b->row < 160 && b->col < 240) {
    b->row += 2;
   } else {
    b->active = 0;
   }
  }
 }

}


